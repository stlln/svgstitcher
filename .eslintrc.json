{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "env": {
    "es6": true,
    "node": true
  },
  "globals": {
  },
  "rules": {
    "no-console": "off",
    "@typescript-eslint/no-var-requires": 0
  }
}
